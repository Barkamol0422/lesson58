def printpowerset(set,setsize):
    power=(int) (setsize**2)
    outer=0
    inner=0
    for outer in range(0,power):
        for inner in range (0,setsize):
            if((outer&(1<<inner))>0):
                print(set[inner],end="")
        print("")
a=str(input('Enter a string: '))
set=[]
for i in range(0,len(a)):
    set.append(a[i])
printpowerset(set,len(set))
  					
